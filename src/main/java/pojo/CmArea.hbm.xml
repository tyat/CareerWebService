<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.CmArea" table="cm_area" schema="career" dynamic-update="true" dynamic-insert="true">
        <id name="aid" type="java.lang.Integer">
            <column name="aid" />
            <generator class="native" />
        </id>
        <property name="aprovince" column="aprovince"/>
        <property name="acity" column="acity"/>
        <set name="cmCompaniesByAid" inverse="true">
            <key>
                <column name="aid"/>
            </key>
            <one-to-many not-found="ignore" class="pojo.CmCompany"/>
        </set>
        <set name="cmIntersByAid" inverse="true">
            <key>
                <column name="aid"/>
            </key>
            <one-to-many not-found="ignore" class="pojo.CmInter"/>
        </set>
        <set name="cmRecruitsByAid" inverse="true">
            <key>
                <column name="aid"/>
            </key>
            <one-to-many not-found="ignore" class="pojo.CmRecruit"/>
        </set>
    </class>
</hibernate-mapping>